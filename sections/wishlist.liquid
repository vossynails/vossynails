{% schema %}
  {
    "name": "Wishlist",
    "class":"wishlist",
    "settings": [
      {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "Padding top",
      "default": 36
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "Padding bottom",
      "default": 36
    }
      
    ],
    "presets": [
    {
      "name": "Wishlist"
    }
  ]
  }
{% endschema %}